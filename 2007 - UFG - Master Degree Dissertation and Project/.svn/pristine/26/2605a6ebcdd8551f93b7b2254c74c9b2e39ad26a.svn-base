\section{Nossa Contribuição}

\begin{frame}
	\frametitle{Nossa Contribuição}
  
		\begin{center}
				\LARGE Nossa principal contribuição é o \textbf{Projeto Arquitetural de um Firewall Pessoal para Dispositivos Móveis}, que se utiliza do paradigma de localização do usuário para carregar preferências e políticas de segurança.
		\end{center}
		
\end{frame}


\begin{frame}[allowframebreaks]
	\frametitle{Nossa Contribuição}
	  
				\begin{itemize}					
					\item \alert{\textbf{[SEGURANÇA]}} \\
					Aumentar o nível de segurança contra os ataques vindo da rede de comunicação;
					\item \alert{\textbf{[CONTROLE]}} \\
					Controlar o acesso aos recursos do dispositivo móvel e as informações;					
					\item \alert{\textbf{[POLÍTICAS E PREFERÊNCIAS]}} \\
					Flexibilidade para que o usuário execute, crie e edite suas preferências e políticas de segurança;					
				\end{itemize}

\end{frame}	

	

\begin{frame}[allowframebreaks]
	\frametitle{Diferencial do Fênix Firewall System}

		\begin{itemize}
					\item \alert{\textbf{[LOCALIZAÇÃO]}} \\
					Carregamento de Políticas e Preferências baseado na localização do usuário;
					\item \alert{\textbf{[INTERATIVIDADE CONTROLADA]}} \\
					Nível de Interatividade controlada pelo usuário;
					\item \alert{\textbf{[META-POLÍTICAS]}} \\
					Meta-políticas exportáveis;
					\item \alert{\textbf{[NOTIFICAÇÃO]}} \\
					Serviço de notificação do usuário via Web;					
		\end{itemize}

\end{frame}